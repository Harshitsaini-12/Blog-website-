<%- include("partials/header"); -%>
<h1>Compose</h1>
<form class="" action="/compose" method="POST">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postname" />
    <label>Post</label>
    <textarea
      class="form-control"
      name="postbody"
      id=""
      cols="30"
      rows="10"
    ></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="submit">Publish</button>
</form>
<%- include("partials/footer"); -%>
